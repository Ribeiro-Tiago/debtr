{
  "name": "debtr",
  "version": "2.1.1",
  "private": false,
  "description": "Simplistic app to help me keep track of what monthly expenses I have left to pay this month",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build:android": "rm -rf android/.gradle && react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle",
    "release:apk": "yarn build:android && cd android && ./gradlew assembleRelease",
    "release:aab": "yarn build:android && cd android && ./gradlew bundleRelease",
    "debug:apk": "yarn build:android && cd android && ./gradlew assembleDebug",
    "update-version": "node infra/update-versions.js",
    "deploy:android": "node infra/deploy-android.js"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/datetimepicker": "^2.6.0",
    "@react-native-community/masked-view": "^0.1.10",
